{
  "name": "eslint-plugin-xeiculy",
  "type": "module",
  "version": "0.0.0",
  "packageManager": "pnpm@10.10.0",
  "description": "XeicuLy's opinionated ESLint rules",
  "author": "XeicuLy",
  "license": "MIT",
  "homepage": "https://github.com/XeicuLy/eslint-plugin-xeiculy#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/XeicuLy/eslint-plugin-xeiculy.git"
  },
  "bugs": "https://github.com/XeicuLy/eslint-plugin-xeiculy/issues",
  "keywords": ["eslint", "eslint-plugin"],
  "sideEffects": false,
  "exports": {
    ".": "./dist/index.js"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": ["dist"],
  "scripts": {
    "biome": "biome check .",
    "biome:fix": "biome check --write .",
    "prettier": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "lint": "pnpm biome && pnpm prettier",
    "lint:fix": "pnpm biome:fix && pnpm prettier:fix",
    "prepare": "husky"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@commitlint/cli": "19.8.0",
    "@types/node": "22.15.3",
    "commitizen": "4.3.1",
    "commitlint-config-gitmoji": "2.3.1",
    "cz-customizable": "7.4.0",
    "husky": "9.1.7",
    "lint-staged": "15.5.1",
    "prettier": "3.5.3",
    "typescript": "5.8.3",
    "vite": "6.3.3"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": ".cz-config.cts"
    }
  },
  "lint-staged": {
    "**/*.{css,js,ts,cjs,mjs,cts,mts,jsx,tsx,json,jsonc}": ["biome check --write --no-errors-on-unmatched"],
    "**/*.{md,html,yaml,yml}": ["prettier --write"]
  },
  "volta": {
    "node": "22.15.0",
    "pnpm": "10.10.0"
  }
}
